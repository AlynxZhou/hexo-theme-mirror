{% block content %}
  {% if page.layout == "help" %}
    {% include "help.njk" %}
  {% else %}
    <div class="content">
      <div class="container">
        <h1>{{ page.title }}</h1>
        {{ page.content }}
        {% if page.layout == "indexTable" %}
          {% include "table.njk" %}
        {% endif %}
      </div>
    </div>
  {% endif %}
{% endblock %}
